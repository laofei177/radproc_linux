.PHONY: clean All

All:
	@echo "----------Building project:[ fmath - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fmath" && "$(MAKE)" -f  "fmath.mk"
	@echo "----------Building project:[ fparameters - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fparameters" && "$(MAKE)" -f  "fparameters.mk"
	@echo "----------Building project:[ fparticle - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fparticle" && "$(MAKE)" -f  "fparticle.mk"
	@echo "----------Building project:[ inout - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/inout" && "$(MAKE)" -f  "inout.mk"
	@echo "----------Building project:[ flosses - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/flosses" && "$(MAKE)" -f  "flosses.mk"
	@echo "----------Building project:[ fluminosities - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fluminosities" && "$(MAKE)" -f  "fluminosities.mk"
	@echo "----------Building project:[ ionTori - Debug ]----------"
	@"$(MAKE)" -f  "ionTori.mk"
clean:
	@echo "----------Cleaning project:[ fmath - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fmath" && "$(MAKE)" -f  "fmath.mk"  clean
	@echo "----------Cleaning project:[ fparameters - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fparameters" && "$(MAKE)" -f  "fparameters.mk"  clean
	@echo "----------Cleaning project:[ fparticle - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fparticle" && "$(MAKE)" -f  "fparticle.mk"  clean
	@echo "----------Cleaning project:[ inout - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/inout" && "$(MAKE)" -f  "inout.mk"  clean
	@echo "----------Cleaning project:[ flosses - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/flosses" && "$(MAKE)" -f  "flosses.mk"  clean
	@echo "----------Cleaning project:[ fluminosities - Debug ]----------"
	@cd "/home/flor/projects/github/radproc_linux/lib/fluminosities" && "$(MAKE)" -f  "fluminosities.mk"  clean
	@echo "----------Cleaning project:[ ionTori - Debug ]----------"
	@"$(MAKE)" -f  "ionTori.mk" clean
